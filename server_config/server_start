sudo -u llzx-usr env \
TOR_PT_MANAGED_TRANSPORT_VER="1" \
TOR_PT_STATE_LOCATION="/var/lib/llzx_usr/" \
TOR_PT_EXI_STDIN_CLOSE="1" \
TOR_PT_SERVER_TRANSPORTS="llzx" \
TOR_PT_SERVER_TRANSPORT_OPTIONS="llzx:iat-mode=1" \
TOR_PT_SERVER_BINDADDR="llzx-0.0.0.0:2222" \
TOR_PT_ORPORT="127.0.0.1:5500" \
llzxproxy -enableLogging -logLevel DEBUG
