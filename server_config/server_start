sudo -u obfs4-usr env \
TOR_PT_MANAGED_TRANSPORT_VER="1" \
TOR_PT_STATE_LOCATION="/var/lib/obfs4_usr/" \
TOR_PT_EXI_STDIN_CLOSE="1" \
TOR_PT_SERVER_TRANSPORTS="obfs4" \
TOR_PT_SERVER_TRANSPORT_OPTIONS="obfs4:iat-mode=1" \
TOR_PT_SERVER_BINDADDR="obfs4-0.0.0.0:2222" \
TOR_PT_ORPORT="127.0.0.1:5500" \
obfs4proxy -enableLogging -logLevel DEBUG
